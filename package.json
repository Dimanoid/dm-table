{
    "name": "@dimanoid/ngx-dm-table",
    "version": "0.7.7",
    "scripts": {
        "build": "ng build lib",
        "build:demo": "ng build --aot --prod --base-href /ngx-dm-table/ demo-app",
        "bl": "ng b lib && ./node_modules/.bin/ngcc -s ./dist && ng s",
        "test": "jest --verbose projects/lib",
        "test:watch": "jest projects/lib --watch --notify",
        "test:ci": "jest --runInBand",
        "cover": "jest projects/lib --coverage",
        "coverall": "jest projects/lib --coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "ng lint",
        "publish": "npm publish dist/lib --access public",
        "up": "cd projects/lib && npm version patch && git add package.json && git commit -m \"version bump [ci skip]\" package*.json && cd ../.. && npm version patch",
        "up:minor": "cd projects/lib && npm version minor && git add package.json && git commit -m \"version bump [ci skip]\" package*.json && cd ../.. && npm version minor"
    },
    "dependencies": {
        "@angular/animations": "~12.2.3",
        "@angular/cdk": "~12.2.3",
        "@angular/common": "~12.2.3",
        "@angular/compiler": "~12.2.3",
        "@angular/core": "~12.2.3",
        "@angular/forms": "~12.2.3",
        "@angular/platform-browser": "~12.2.3",
        "@angular/platform-browser-dynamic": "~12.2.3",
        "@angular/router": "~12.2.3",
        "ng-zorro-antd": "~12.0.1",
        "rxjs": "~7.3.0",
        "tslib": "~2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "~12.1.1",
        "@angular-devkit/build-angular": "~12.2.3",
        "@angular-devkit/build-ng-packagr": "~0.1002.0",
        "@angular/cli": "~12.2.3",
        "@angular/compiler-cli": "~12.2.3",
        "@angular/language-service": "~12.2.3",
        "@ngneat/spectator": "~8.0.4",
        "@types/jest": "~27.0.1",
        "@types/node": "~16.7.8",
        "@typescript-eslint/eslint-plugin": "~4.30.0",
        "@typescript-eslint/parser": "~4.30.0",
        "codelyzer": "~6.0.2",
        "eslint": "~7.32.0",
        "is-windows": "~1.0.2",
        "jest": "~27.1.0",
        "jest-preset-angular": "~9.0.7",
        "ng-packagr": "~12.2.1",
        "nz-tslint-rules": "~0.901.2",
        "resize-observer-polyfill": "~1.5.1",
        "ts-node": "~10.2.1",
        "tsickle": "~0.43.0",
        "tslint": "^6.1.3",
        "typescript": "~4.2.3"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ]
    }
}
