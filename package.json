{
    "name": "@dimanoid/ngx-dm-table",
    "version": "14.0.5",
    "scripts": {
        "build": "ng build lib",
        "build:demo": "ng build --aot --prod --base-href /ngx-dm-table/ demo-app",
        "bl": "ng b lib && ./node_modules/.bin/ngcc -s ./dist && ng s",
        "test": "jest --verbose projects/lib",
        "test:watch": "jest projects/lib --watch --notify",
        "test:ci": "jest --runInBand",
        "cover": "jest projects/lib --coverage",
        "coverall": "jest projects/lib --coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "ng lint",
        "publish": "npm publish dist/lib --access public",
        "up": "cd projects/lib && npm version patch && cd ../.. && npm version patch &&  git add package*.json && git commit -m \"version bump [ci skip]\"",
        "up:minor": "cd projects/lib && npm version minor && git add package.json && git commit -m \"version bump [ci skip]\" package*.json && cd ../.. && npm version minor"
    },
    "dependencies": {
        "@angular/animations": ">=14, <15",
        "@angular/cdk": ">=14, <15",
        "@angular/common": ">=14, <15",
        "@angular/compiler": ">=14, <15",
        "@angular/core": ">=14, <15",
        "@angular/forms": ">=14, <15",
        "@angular/platform-browser": ">=14, <15",
        "@angular/platform-browser-dynamic": ">=14, <15",
        "@angular/router": ">=14, <15",
        "ng-zorro-antd": ">=14, <15",
        "rxjs": ">=7.5.2",
        "tslib": ">=2.3.1",
        "zone.js": ">=0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": ">=14, <15",
        "@angular-devkit/build-angular": ">=14, <15",
        "@angular/cli": ">=14, <15",
        "@angular/compiler-cli": ">=14, <15",
        "@angular/language-service": ">=14, <15",
        "@ngneat/spectator": "~10.0.0",
        "@types/estree": "^1.0.0",
        "@types/jest": "~27.4.0",
        "@types/node": "~17.0.14",
        "@typescript-eslint/eslint-plugin": "~5.10.2",
        "@typescript-eslint/parser": "~5.10.2",
        "eslint": "~8.8.0",
        "is-windows": "~1.0.2",
        "jest": ">=28",
        "jest-preset-angular": "~11.1.0",
        "ng-packagr": ">14, <15",
        "nz-tslint-rules": "~0.901.2",
        "ts-node": "~10.4.0",
        "tslint": "^6.1.3",
        "typescript": ">=4.8.4"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ]
    }
}
