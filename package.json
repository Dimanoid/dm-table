{
    "name": "@dimanoid/ngx-dm-table",
    "version": "0.7.13",
    "scripts": {
        "build": "ng build lib",
        "build:demo": "ng build --aot --prod --base-href /ngx-dm-table/ demo-app",
        "bl": "ng b lib && ./node_modules/.bin/ngcc -s ./dist && ng s",
        "test": "jest --verbose projects/lib",
        "test:watch": "jest projects/lib --watch --notify",
        "test:ci": "jest --runInBand",
        "cover": "jest projects/lib --coverage",
        "coverall": "jest projects/lib --coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "ng lint",
        "publish": "npm publish dist/lib --access public",
        "up": "cd projects/lib && npm version patch && git add package.json && git commit -m \"version bump [ci skip]\" package*.json && cd ../.. && npm version patch",
        "up:minor": "cd projects/lib && npm version minor && git add package.json && git commit -m \"version bump [ci skip]\" package*.json && cd ../.. && npm version minor"
    },
    "dependencies": {
        "@angular/animations": "~13.2.0",
        "@angular/cdk": "~13.2.0",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/forms": "~13.2.0",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "ng-zorro-antd": "~13.0.1",
        "rxjs": "~7.5.2",
        "tslib": "~2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "~13.0.3",
        "@angular-devkit/build-angular": "~13.2.1",
        "@angular-devkit/build-ng-packagr": "~0.1002.0",
        "@angular/cli": "~13.2.1",
        "@angular/compiler-cli": "~13.2.0",
        "@angular/language-service": "~13.2.0",
        "@ngneat/spectator": "~10.0.0",
        "@types/jest": "~27.4.0",
        "@types/node": "~17.0.14",
        "@typescript-eslint/eslint-plugin": "~5.10.2",
        "@typescript-eslint/parser": "~5.10.2",
        "codelyzer": "~6.0.2",
        "eslint": "~8.8.0",
        "is-windows": "~1.0.2",
        "jest": "~27.4.7",
        "jest-preset-angular": "~11.1.0",
        "ng-packagr": "~13.2.1",
        "nz-tslint-rules": "~0.901.2",
        "ts-node": "~10.4.0",
        "tsickle": "~0.46.0",
        "tslint": "^6.1.3",
        "typescript": "~4.4.4"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ]
    }
}
