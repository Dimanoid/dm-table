{
    "name": "@dimanoid/ngx-dm-table",
    "version": "15.0.0",
    "scripts": {
        "build": "ng build lib",
        "build:demo": "ng build --configuration=production --base-href /ngx-dm-table/ demo-app",
        "bl": "ng b lib && ./node_modules/.bin/ngcc -s ./dist && ng s",
        "test": "jest --verbose projects/lib",
        "test:watch": "jest projects/lib --watch --notify",
        "test:ci": "jest --runInBand",
        "cover": "jest projects/lib --coverage",
        "coverall": "jest projects/lib --coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "ng lint",
        "publish": "npm publish dist/lib --access public",
        "up": "cd projects/lib && npm version patch && cd ../.. && npm version patch &&  git add package*.json **/package*.json && npm run commit",
        "up:minor": "cd projects/lib && npm version minor && cd ../.. && npm version minor &&  git add package*.json **/package*.json && npm run commit",
        "commit": "git commit -m \"version $npm_package_version\""
    },
    "dependencies": {
        "@angular/animations": ">=15, <16",
        "@angular/cdk": ">=15, <16",
        "@angular/common": ">=15, <16",
        "@angular/compiler": ">=15, <16",
        "@angular/core": ">=15, <16",
        "@angular/forms": ">=15, <16",
        "@angular/platform-browser": ">=15, <16",
        "@angular/platform-browser-dynamic": ">=15, <16",
        "@angular/router": ">=15, <16",
        "ng-zorro-antd": ">=15, <16",
        "rxjs": ">=7.8.0",
        "tslib": ">=2.5.0",
        "zone.js": ">=0.13.0"
    },
    "devDependencies": {
        "@angular-builders/jest": ">=15, <16",
        "@angular-devkit/build-angular": ">=15, <16",
        "@angular/cli": ">=15, <16",
        "@angular/compiler-cli": ">=15, <16",
        "@angular/language-service": ">=15, <16",
        "@ngneat/spectator": "~14.0.0",
        "@types/estree": "^1.0.0",
        "@types/jest": "~29.5.0",
        "@types/node": "~18.15.11",
        "@typescript-eslint/eslint-plugin": "~5.58.0",
        "@typescript-eslint/parser": "~5.58.0",
        "eslint": "~8.38.0",
        "is-windows": "~1.0.2",
        "jest": ">=29",
        "jest-preset-angular": "~13.0.1",
        "ng-packagr": ">15, <16",
        "nz-tslint-rules": "~0.901.2",
        "ts-node": "~10.9.1",
        "tslint": "^6.1.3",
        "typescript": ">=4.8.2 and <5.0.0"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ]
    }
}
