{
    "name": "@dimanoid/ngx-dm-table",
    "version": "16.0.3",
    "scripts": {
        "build": "ng build lib",
        "build:demo": "ng build --configuration=production --base-href /ngx-dm-table/ demo-app",
        "bl": "ng b lib && ./node_modules/.bin/ngcc -s ./dist && ng s",
        "test": "jest --verbose projects/lib",
        "test:watch": "jest projects/lib --watch --notify",
        "test:ci": "jest --runInBand",
        "cover": "jest projects/lib --coverage",
        "coverall": "jest projects/lib --coverage && cat ./coverage/lcov.info | coveralls",
        "lint": "ng lint",
        "publish": "npm publish dist/lib --access public",
        "up": "cd projects/lib && npm version patch && cd ../.. && npm version patch &&  git add package*.json **/package*.json && npm run commit",
        "up:minor": "cd projects/lib && npm version minor && cd ../.. && npm version minor &&  git add package*.json **/package*.json && npm run commit",
        "commit": "git commit -m \"version $npm_package_version\""
    },
    "dependencies": {
        "@angular/animations": ">=16, <17",
        "@angular/cdk": ">=16, <17",
        "@angular/common": ">=16, <17",
        "@angular/compiler": ">=16, <17",
        "@angular/core": ">=16, <17",
        "@angular/forms": ">=16, <17",
        "@angular/platform-browser": ">=16, <17",
        "@angular/platform-browser-dynamic": ">=16, <17",
        "@angular/router": ">=16, <17",
        "ng-zorro-antd": ">=16, <17",
        "rxjs": ">=7.8.1",
        "tslib": ">=2.5.3",
        "zone.js": ">=0.13.1"
    },
    "devDependencies": {
        "@angular-builders/jest": ">=16, <17",
        "@angular-devkit/build-angular": ">=16, <17",
        "@angular/cli": ">=16, <17",
        "@angular/compiler-cli": ">=16, <17",
        "@angular/language-service": ">=16, <17",
        "@ngneat/spectator": "~15.0.1",
        "@types/estree": "^1.0.1",
        "@types/jest": "~29.5.2",
        "@types/node": "~20.3.1",
        "@typescript-eslint/eslint-plugin": "~5.59.11",
        "@typescript-eslint/parser": "~5.59.11",
        "eslint": "~8.42.0",
        "is-windows": "~1.0.2",
        "jest": ">=29",
        "jest-preset-angular": "~13.1.1",
        "ng-packagr": ">15, <17",
        "nz-tslint-rules": "~0.901.2",
        "ts-node": "~10.9.1",
        "tslint": "^6.1.3",
        "typescript": ">=4.8.2 and <5.0.0"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setup-jest.ts"
        ]
    }
}
